{
  "functions": {
    "api/admin.py": {
      "runtime": "python3.9"
    }
  },
  "rewrites": [
    {
      "source": "/admin/(.*)",
      "destination": "/api/admin.py"
    }
  ],
  "outputDirectory": "public_archive",
  "cleanUrls": true,
  "headers": [
    {
      "source": "/sw.js",
      "headers": [
        {
          "key": "Cache-Control",
          "value": "no-cache, no-store, must-revalidate"
        }
      ]
    }
  ]
}